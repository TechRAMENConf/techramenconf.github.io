---
import BaseLayout from "@/layouts/BaseLayout.astro";
import Abstract from "@/components/landing/Abstract.astro";
import Information from "@/components/landing/Information.astro";
import Speakers from "@/components/landing/Speakers.astro";
import SponsorshipOffers from "@/components/landing/SponsorshipOffers.astro"
import ExternalLinks from "@/components/landing/ExternalLinks.astro"
import Personalsponsors from "@/components/landing/PersonalSponsors.astro";
import Corporatesponsors from "@/components/landing/CorporateSponsors.astro";
import EndingMovie from "@/components/landing/EndingMovie.astro";
import StaffList from "@/components/landing/StaffList.astro";
---
<BaseLayout>
  <Abstract />
  <ExternalLinks />
  <Information />
  <Speakers />
  <SponsorshipOffers />
  <Personalsponsors />
  <Corporatesponsors />
  <StaffList />
  <section class="section-colored section-secondary overflow-hidden relative control-panel control-panel-screw-enhanced">
    <div class="container">
      <h2 class="text-center mb-4">
        <span class="title-plate title-plate-xl">2024 ENDING MOVIE</span>
      </h2>
      <div class="max-w-3xl mx-auto">
        <EndingMovie />
      </div>
    </div>
  </section>
</BaseLayout>
